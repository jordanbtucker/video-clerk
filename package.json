{
  "name": "video-clerk",
  "version": "2.3.0",
  "description": "A CLI for organizing movie and TV show files",
  "bin": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^14 || ^16 || >=18",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanbtucker/video-clerk.git"
  },
  "homepage": "https://github.com/jordanbtucker/video-clerk#readme",
  "bugs": {
    "url": "https://github.com/jordanbtucker/video-clerk/issues"
  },
  "author": {
    "name": "Jordan Tucker",
    "email": "jordanbtucker@gmail.com",
    "url": "https://github.com/jordanbtucker"
  },
  "license": "MIT",
  "keywords": [
    "movies",
    "tv",
    "shows",
    "episodes",
    "rename",
    "organize",
    "plex"
  ],
  "scripts": {
    "coverage": "run-s -c tap tap:coverage",
    "eslint": "eslint --fix .",
    "lint": "run-s eslint prettier",
    "prepare": "husky install",
    "prettier": "prettier --loglevel warn -w .",
    "preversion": "npm test",
    "start": "node lib",
    "tap": "tap -R spec test",
    "tap:coverage": "tap --coverage-report html",
    "test": "run-s lint tap",
    "version": "git add CHANGELOG.md"
  },
  "dependencies": {
    "conf": "^10.2.0",
    "cross-fetch": "^3.1.5",
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "tap": "^16.3.0"
  }
}
